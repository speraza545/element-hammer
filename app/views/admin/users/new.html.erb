<h1>admin new</h1>
<h1>Sign up</h1>
<%= email_taken_output %>
<%= not_admin_output %>
<%= form_for @user do |f|%>
    <%= f.text_field :name, placeholder: "Name", class: "form_field" %>

    <%= f.text_field :email, placeholder: "Email", class: "form_field" %>

    <%= f.text_field :password, placeholder: "Password", class: "form_field" %>

    <%= f.number_field :code, placeholder: "Code", class: "form_field" %>

    <%= f.submit class: "btn btn-primary" %>
<% end %>